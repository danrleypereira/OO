package aula2;
import java.util.Scanner;

public class QualSexo {

	public static void main(String[] args) {
		//Declara objeto ler
		Scanner ler = new Scanner(System.in);
		//declara a string sexo
		String[] sexo = {"MACHO", "FÊMEA"};
		//string do usuário
		String usuarioDeclaracao = ler.next();
		//compara a string inserida com MACHO e imprime MACHO ou FEMEA
		System.out.println(usuarioDeclaracao.equalsIgnoreCase(sexo[0]) ? sexo[0] : sexo[1]);
		
		ler.close();
		

	}

}
